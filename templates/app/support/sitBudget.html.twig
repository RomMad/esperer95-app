{% extends "model/collapseCard.html.twig" %}

{% set id = "sit_budget" %}

{% block card_title %}Situation budgétaire{% endblock %}

{% block card_body %}

<div class="form-group row">
	<div class="col-md-10 col-lg-6 mx-auto">
		<table class="table table-striped table-hover">
			<thead>
				<tr>
					<th scope="col" class="align-middle th-w-140">Situation budgétaire du groupe</th>
					<th scope="col" class="align-middle th-w-100">Montant</th>
				</tr>
			</thead>
			<tbody>
				<tr>
					<td>Total ressources</td>
					<td class="pr-3 text-right">
						<span id="ressourcesGroupAmt">{% if support.sitBudgetGroup.ressourcesGroupAmt is defined %}
							{{ support.sitBudgetGroup.ressourcesGroupAmt }}{% else %}0{% endif %}</span> €</td>
				</tr>
				<tr>
					<td>Total charges</td>
					<td class="pr-3 text-right">
						<span id="chargesGroupAmt">{% if support.sitBudgetGroup.chargesGroupAmt is defined %}
							{{ support.sitBudgetGroup.chargesGroupAmt }}{% else %}0{% endif %}</span> €</td>
				</tr>
				<tr>
					<td>Total remboursement mensuel</td>
					<td class="pr-3 text-right">
						<span id="repaymentGroupAmt">{% if support.sitBudgetGroup.monthlyRepaymentAmt is defined %}
							{{ support.sitBudgetGroup.monthlyRepaymentAmt }}{% else %}0{% endif %}</span> €</td>
				</tr>
				<tr class="table-dark text-dark font-weight-bold">
					<td>Reste à vivre</td>
					<td class="pr-3 text-right">
						<span id="budgetBalanceGroupAmt">{% if support.sitBudgetGroup.budgetBalanceAmt is defined %}
							{{ support.sitBudgetGroup.budgetBalanceAmt }}{% else %}0{% endif %}</span> €</td>
				</tr>
				<tr>
					<td>Total dettes</td>
					<td class="pr-3 text-right">
						<span id="debtsGroupAmt">{% if support.sitBudgetGroup.debtsGroupAmt is defined %}
							{{ support.sitBudgetGroup.debtsGroupAmt }}{% else %}0{% endif %}</span> €</td>
				</tr>
			</tbody>
		</table>
	</div>
</div>

